{
  "name": "fuse-financial-verification-agent",
  "version": "1.0.0",
  "description": "A Node.js conversational AI agent for secure financial verification, built with a robust state machine architecture.",
  "main": "src/simulator.js",
  "type": "module",
  "scripts": {
    "test": "node test-agent.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start": "node src/simulator.js",
    "dev": "nodemon src/simulator.js",
    "setup": "./setup.sh",
    "monitoring": "./start-monitoring.sh",
    "evaluation": "./run-evaluation.sh",
    "pipeline": "./run-pipeline.sh",
    "generate:mock": "node scripts/generate-mock-data.js",
    "evaluate": "node scripts/evaluate-multiple-datasets.js",
    "evaluate:all": "node scripts/evaluate-multiple-datasets.js",
    "generate:successful": "node scripts/generate-mock-data.js --count 10 --type successful",
    "generate:failure": "node scripts/generate-mock-data.js --count 5 --type identity_failure",
    "generate:mixed": "node scripts/generate-mock-data.js --count 20 --type mixed --llm",
    "analyze": "node scripts/analyze-conversations.js",
    "dashboard": "node scripts/conversation-dashboard.js",
    "simulator:generated": "node src/simulator-with-generated-data.js"
  },
  "keywords": [
    "ai",
    "verification",
    "financial",
    "conversational",
    "agent"
  ],
  "author": "alejoartia",
  "license": "MIT",
  "dependencies": {
    "@langchain/openai": "^0.0.14",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "langchain": "^0.0.208"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
